{
  "name": "@nde/next-gen-tools",
  "version": "1.0.1",
  "description": "Electron app with floating DevTools",
  "main": "./out/main/index.js",
  "author": "Next Dev Team <info@next-dev.team>",
  "homepage": "https://github.com/next-dev-team/next-gen#readme",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "prebuild:copy": "node scripts/prebuild-copy.js",
    "start": "node ../../node_modules/electron-vite/bin/electron-vite.js preview",
    "dev": "npm run prebuild:copy && node ../../node_modules/electron-vite/bin/electron-vite.js dev",
    "web:dev": "node ../../node_modules/vite/bin/vite.js --config vite.web.config.mjs",
    "build": "node ../../node_modules/electron-vite/bin/electron-vite.js build && npm run prebuild:copy",
    "web:build": "node ../../node_modules/vite/bin/vite.js build --config vite.web.config.mjs",
    "web:preview": "node ../../node_modules/vite/bin/vite.js preview --config vite.web.config.mjs --port 5176",
    "postinstall": "node scripts/postinstall.js || echo 'postinstall failed (non-fatal)'",
    "build:win": "pnpm run build && node ../../node_modules/electron-builder/out/cli/cli.js --win -c.npmRebuild=false",
    "build:mac": "pnpm run build && node ../../node_modules/electron-builder/out/cli/cli.js --mac -c.npmRebuild=false",
    "build:linux": "pnpm run build && node ../../node_modules/electron-builder/out/cli/cli.js --linux -c.npmRebuild=false",
    "test": "node ../../node_modules/electron-vite/bin/electron-vite.js build && node ../../node_modules/@playwright/test/cli.js test",
    "mcp:scrum": "node scripts/scrum-mcp-server.js",
    "mcp:sse": "node scripts/scrum-mcp-server.js",
    "dev:full": "node ../../node_modules/concurrently/dist/bin/concurrently.js \"pnpm run dev\" \"pnpm run mcp:sse\""
  },
  "devDependencies": {
    "@kubb/cli": "^4.12.15",
    "@kubb/core": "^4.12.15",
    "@kubb/plugin-client": "^4.12.15",
    "@kubb/plugin-oas": "^4.12.15",
    "@kubb/plugin-react-query": "^4.12.15",
    "@kubb/plugin-ts": "^4.12.15",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@vitejs/plugin-react": "^5.1.2",
    "code-inspector-plugin": "^1.3.4",
    "concurrently": "^9.2.1",
    "electron": "28.3.3",
    "electron-builder": "^24.9.1",
    "electron-vite": "^5.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.17",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "@nde/llm": "workspace:*",
    "@ant-design/cssinjs": "^2.0.1",
    "@ant-design/icons": "^6.1.0",
    "@ghostery/adblocker-electron": "^2.5.0",
    "@measured/puck": "^0.20.2",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.7",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.71.10",
    "@tanstack/react-query-devtools": "^5.71.10",
    "antd": "^6.1.1",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "conf": "^10.2.0",
    "cross-fetch": "^4.1.0",
    "electron-updater": "^6.3.0",
    "electron-store": "^8.1.0",
    "lucide-react": "^0.562.0",
    "node-pty": "^1.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vconsole": "^3.15.1",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "build": {
    "appId": "com.float-devtools-app.app",
    "productName": "next-gen-tools",
    "publish": [
      {
        "provider": "github",
        "owner": "next-dev-team",
        "repo": "next-gen"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "scripts/**/*",
      "package.json",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "scripts",
        "to": "scripts"
      },
      {
        "from": "../../scripts/run-generator.ts",
        "to": "scripts/run-generator.ts"
      },
      {
        "from": "../../turbo",
        "to": "turbo",
        "filter": [
          "**/*",
          "!**/node_modules/**"
        ]
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "maintainer": "Next Dev Team <info@next-dev.team>",
      "category": "Development"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "signAndEditExecutable": false
    }
  }
}
